generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
  runtime  = "deno"
}

datasource db {
  provider = "sqlite"
}

model ipfact {
  id   Int      @id @default(autoincrement())
  ip   String
  seen DateTime

  @@unique([ip, seen])
}

model knownlog {
  id   Int    @id @default(autoincrement())
  file String @unique
}
